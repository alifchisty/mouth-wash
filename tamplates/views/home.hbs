  <script>
let slides = document.getElementsByClassName("slide");
let navlinks = document.getElementsByClassName("dot");
let currentSlide = 0;
let slideInterval;

// Function to change the slide
function changeSlide(moveTo) {
  if (moveTo >= slides.length) {
    moveTo = 0;
  }
  if (moveTo < 0) {
    moveTo = slides.length - 1;
  }

  slides[currentSlide].classList.toggle("active");
  navlinks[currentSlide].classList.toggle("activeDot");
  slides[moveTo].classList.toggle("active");
  navlinks[moveTo].classList.toggle("activeDot");
  currentSlide = moveTo;
}

// Function to start the auto-slide
function startSlideInterval() {
  slideInterval = setInterval(() => {
    changeSlide(currentSlide + 1);
  }, 2500);
}

// Function to stop the auto-slide
function stopSlideInterval() {
  clearInterval(slideInterval);
}

// Function to restart the slide interval after stopping
function restartSlideInterval() {
  stopSlideInterval();
  startSlideInterval();
}

// Start the slide interval on page load
startSlideInterval();

// Event listeners for manual controls
document.getElementById("button-next").addEventListener("click", () => {
  changeSlide(currentSlide + 1);
  stopSlideInterval(); // Stop auto-slide on manual navigation
});

document.getElementById("button-prev").addEventListener("click", () => {
  changeSlide(currentSlide - 1);
  stopSlideInterval(); // Stop auto-slide on manual navigation
});

document.querySelectorAll(".dot").forEach((bullet, bulletIndex) => {
  bullet.addEventListener("click", () => {
    if (currentSlide !== bulletIndex) {
      changeSlide(bulletIndex);
      stopSlideInterval(); // Stop auto-slide on manual navigation
    }
  });
});

// Stop auto-slide on image click
document.querySelectorAll(".slide img").forEach((image) => {
  image.addEventListener("click", () => {
    stopSlideInterval(); // Stop auto-slide on image click
  });
});

// VIP link handler
document.querySelector('.vip-link').addEventListener('click', (event) => {
  event.preventDefault();

  const vipuserId = localStorage.getItem('userId');
  if (vipuserId) {
    window.location.href = `/vip?vipuserId=${vipuserId}`;
  } else {
    alert('Invalid user ID');
  }
});
  </script>
